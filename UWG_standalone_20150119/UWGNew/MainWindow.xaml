<Window x:Class="UWG.UWGInputs"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:chartingToolkit="clr-namespace:System.Windows.Controls.DataVisualization.Charting;assembly=System.Windows.Controls.DataVisualization.Toolkit"
        xmlns:datavis="clr-namespace:System.Windows.Controls.DataVisualization;assembly=System.Windows.Controls.DataVisualization.Toolkit"
        Title="Urban Weather Generator" Name="UWGForm" Icon="Images/CENSAM Poster_diagrams2_3-21.png" Height="750" MinWidth="10" Width="1050" MinHeight="10" MaxWidth="1500" ResizeMode="CanResizeWithGrip" Background="#f6f6f6">

    <Window.Resources>
        <Style TargetType="{x:Type Window}">
            <Setter Property="FontSize" Value="12" />
            <Setter Property="Margin" Value="0,0,0,0" />
            <Setter Property="Width" Value="auto" />
            <Setter Property="Height" Value="auto" />
        </Style>
        <!--Label -->
        <Style TargetType="Label">
            <Setter Property="Height" Value="26" />
            <!-- total height is 30-->
            <Setter Property="Width" Value="260" />
            <Setter Property="Margin" Value="4" />
            <Setter Property="HorizontalAlignment" Value="Left" />
            <Setter Property="VerticalAlignment" Value="Center" />
        </Style>
        <Style  x:Key="tip" TargetType="Label">
            <Setter Property="Height" Value="26" />
            <Setter Property="Width" Value="240" />
            <Setter Property="Margin" Value="4" />
            <Setter Property="FontWeight" Value="Bold" />
            <Setter Property="Cursor" Value="Help" />
            <Setter Property="Focusable" Value="True" />
        </Style>
        <Style x:Key="advSettingLabel" TargetType="{x:Type Label}">
            <Setter Property="Foreground" Value="gray" />
            <Setter Property="Width" Value="120" />
            <Setter Property="Height" Value="26" />
            <Setter Property="Margin" Value="4" />
        </Style>
        <Style x:Key="templateValues" TargetType="{x:Type Label}">
            <Setter Property="Foreground" Value="gray" />
            <Setter Property="Width" Value="260" />
            <Setter Property="Height" Value="26" />
            <Setter Property="Margin" Value="4" />
            <Setter Property="VerticalAlignment" Value="Center" />
            <Setter Property="HorizontalAlignment" Value="Left" />
            
        </Style>
        <!--Heading-->
        <Style  x:Key="heading" TargetType="Label">
            <Setter Property="Height" Value="26" />
            <Setter Property="Width" Value="240" />
            <Setter Property="Margin" Value="4" />
            <Setter Property="Foreground" Value="Gray" />
            <Setter Property="HorizontalAlignment" Value="Left" />
        </Style>
        <!--Tab Control -->
        <Style TargetType="TabControl">
            <Setter Property="TabStripPlacement" Value="Top" />
            <Setter Property="BorderThickness" Value="0" />
            <Setter Property="Background" Value="#f6f6f6" />
        </Style>
        <!-- Tab Item-->
        <Style TargetType="TabItem">
            <Setter Property="Width" Value="130" />
            <Setter Property="BorderThickness" Value="0" />
            <Setter Property="Margin" Value="0" />
            <Setter Property="VerticalAlignment" Value="Center" />
        </Style>
        <!-- Stack Panel -->
        <!-- Left Panel-->
        <Style x:Key="PanelLeft" TargetType="StackPanel">
            <Setter Property="HorizontalAlignment" Value="Left" />
            <Setter Property="VerticalAlignment" Value="Top" />
            <Setter Property="Margin" Value="0 8 0 0" />
        </Style>
        <!--Right Panel-->
        <Style x:Key="PanelRight" TargetType="StackPanel">
            <Setter Property="HorizontalAlignment" Value="Left" />
            <Setter Property="VerticalAlignment" Value="Top" />
            <Setter Property="Margin" Value="0 8 0 0" />
        </Style>
        <!--Level 2 stackpanel-->
        <Style x:Key="level2" TargetType="StackPanel">
            <Setter Property="Margin" Value="20 0 10 0" />
        </Style>
        <!-- stackpanel level 1-->
        <Style x:Key="level1" TargetType="StackPanel">
            <Setter Property="Opacity" Value="70" />
        </Style>
        <!--Grid level 1-->
        <Style x:Key="level1Grid" TargetType="Grid">
            <Setter Property="Opacity" Value="70" />
            <Setter Property="Margin" Value="0 4 0 0" />
        </Style>
        <!--Text Box -->
        <Style TargetType="TextBox">
            <Setter Property="Height" Value="26" />
            <!-- total height is 30-->
            <Setter Property="Margin" Value="4" />
            <Setter Property="Width" Value="120" />
            <Setter Property="HorizontalAlignment" Value="Left" />
        </Style>
        <Style TargetType="TextBlock">
            <Setter Property="Height" Value="26" />
            <!-- total height is 30-->
            <Setter Property="Margin" Value="4" />
            <Setter Property="HorizontalAlignment" Value="Left" />
        </Style>
        <Style x:Key="filePath" TargetType="TextBox">
            <Setter Property="Height" Value="26" />
            <!-- total height is 30-->
            <Setter Property="Margin" Value="4" />
            <Setter Property="Width" Value="380" />
        </Style>
        <Style x:Key="separator_left" TargetType="{x:Type Separator}">
            <Setter Property="Margin" Value="0 8 0 8" />
        </Style>
        <Style x:Key="separator_right" TargetType="{x:Type Separator}">
            <Setter Property="Margin" Value="0 8 0 42" />
        </Style>
        <Style x:Key="advSetting" TargetType="{x:Type TextBox}">
            <Setter Property="Foreground" Value="gray" />
            <Setter Property="Width" Value="120" />
            <Setter Property="Height" Value="26" />
            <Setter Property="Margin" Value="4" />
            <Setter Property="FontWeight" Value="Normal" />
            <Setter Property="HorizontalAlignment" Value="Left" />
        </Style>
        <!-- Combobox-->
        <Style TargetType="ComboBox">
            <Setter Property="Width" Value="120" />
            <Setter Property="Height" Value="26" />
            <Setter Property="Margin" Value="4" />
            <Setter Property="VerticalContentAlignment" Value="Center" />
            <Setter Property="HorizontalAlignment" Value="Left" />
        </Style>
        <!-- Button-->
        <Style TargetType="Button">
            <Setter Property="Width" Value="120" />
            <Setter Property="Height" Value="26" />
            <Setter Property="Margin" Value="4" />
            <Setter Property="Padding" Value="0" />
            <Setter Property="BorderThickness" Value="0" />
            <Setter Property="BorderThickness" Value="0" />
        </Style>
        <Style x:Key="specialButton" TargetType="Button">
            <Style.Triggers>
                <Trigger Property="IsEnabled" Value="true">
                    <Setter Property="Background" Value="#FF00B4B4" />
                    <Setter Property="BorderBrush" Value="White" />
                    <Setter Property="BorderThickness" Value="0" />
                </Trigger>
                <Trigger Property="IsEnabled" Value="false">
                    <Setter Property="Background" Value="#f6f6f6" />
                    <Setter Property="BorderBrush" Value="#FF00B4B4" />
                    <Setter Property="BorderThickness" Value="1" />
                    <Setter Property="Foreground" Value="Gray" />
                </Trigger>
            </Style.Triggers>
            <Setter Property="Width" Value="120" />
            <Setter Property="Height" Value="26" />
            <Setter Property="Margin" Value="4" />
            <Setter Property="Padding" Value="0" />
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type Button}">
                        <Border CornerRadius="4" Background="{TemplateBinding Background}">
                            <Grid>
                                <ContentPresenter x:Name="MyContentPresenter" Content="{TemplateBinding Content}" HorizontalAlignment="Center" VerticalAlignment="Center" />
                            </Grid>
                        </Border>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>
        <!--scroll viewer -->
        <Style TargetType="ScrollViewer">
            <Setter Property="Background" Value="White" />
            <Setter Property="BorderThickness" Value="0" />
            <Setter Property="Padding" Value=" 4" />
        </Style>
        <!--Expander-->
        <Style TargetType="Expander">
            <Setter Property="HorizontalAlignment" Value="Stretch" />
            <Setter Property="HorizontalContentAlignment" Value="Stretch" />
            <Setter Property="VerticalAlignment" Value="Bottom" />
            <Setter Property="Grid.ColumnSpan" Value="2" />
        </Style>
        <DataTemplate x:Key="expanderHeader">
            <TextBlock Text="{Binding}" FontWeight="Bold" Foreground="Gray"/>
        </DataTemplate>
        <Style x:Key="iconImage"  TargetType="Image">
            <Setter Property="Height" Value="16" />
            <Setter Property="Margin" Value="4" />
        </Style>
        <Style x:Key="SectionHeader" TargetType="Label">
            <Setter Property="FontWeight" Value="Bold" />
            <Setter Property="VerticalAlignment" Value="Center" />
            <Setter Property="Padding" Value="8 0 0 0" />
        </Style>
        <!--start uwg button styling-->
        <Style  x:Key="validate" TargetType="{x:Type Button}">
            <Setter Property="IsEnabled" Value="false" />
            <Setter Property="Width" Value="120" />
            <Setter Property="Height" Value="26" />
            <Setter Property="Margin" Value="4" />
            <Style.Triggers>
                <!-- Require the controls to be valid in order to press button -->
                <MultiDataTrigger>
                    <MultiDataTrigger.Conditions>
                        <Condition Binding="{Binding ElementName=epwFileEmpty, Path=(Validation.HasError)}" Value="false" />
                        <Condition Binding="{Binding ElementName=xmlFileEmpty, Path=(Validation.HasError)}" Value="false" />
                        <Condition Binding="{Binding ElementName=simuStartMonth, Path=(Validation.HasError)}" Value="false" />
                        <Condition Binding="{Binding ElementName=simuStartDay, Path=(Validation.HasError)}" Value="false" />
                        <Condition Binding="{Binding ElementName=simuDuration, Path=(Validation.HasError)}" Value="false" />
                    </MultiDataTrigger.Conditions>
                    <Setter Property="IsEnabled" Value="true" />
                </MultiDataTrigger>
            </Style.Triggers>
        </Style>
        <!-- top layer tabs -->
        <Style x:Key="idd" TargetType="MenuItem">
            <Style.Triggers>
                <Trigger Property="IsEnabled" Value="true">
                </Trigger>
                <Trigger Property="IsEnabled" Value="false">
                </Trigger>
            </Style.Triggers>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type MenuItem}">
                        <Grid Background="{TemplateBinding Background}">
                            <Label Content="{TemplateBinding Header}" Width="{TemplateBinding Width}" IsEnabled="{TemplateBinding IsEnabled}"/>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
            <Setter Property="Width" Value="130" />
            <Setter Property="VerticalAlignment" Value="Center" />
            <Setter Property="Background" Value="#f6f6f6" />
        </Style>
        <!-- first layer tabs -->
        <Style x:Key="sec" TargetType="TabItem">
            <Style.Triggers>
                <Trigger Property="IsSelected" Value="true">
                    <Setter Property="Background" Value="White" />
                    <Setter Property="BorderBrush" Value="White" />
                    <Setter Property="BorderThickness" Value="0" />
                </Trigger>
                <Trigger Property="IsSelected" Value="false">
                    <Setter Property="Background" Value="#f6f6f6" />
                    <Setter Property="BorderBrush" Value="Transparent" />
                </Trigger>
            </Style.Triggers>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="{x:Type TabItem}">
                        <Grid UseLayoutRounding="True">
                            <Border CornerRadius="4,4,0,0" Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="4">
                                <Grid Background="{TemplateBinding Background}">
                                    <Label Content="{TemplateBinding Header}" />
                                </Grid>
                            </Border>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
            <Setter Property="Width" Value="130" />
            <Setter Property="BorderThickness" Value="2" />
            <Setter Property="BorderBrush" Value="White" />
            <Setter Property="Height" Value="auto" />
            <Setter Property="VerticalAlignment" Value="Center" />
        </Style>
        <Style x:Key="lineStyle" TargetType="Line">
            <Setter Property="Stroke" Value="Gray" />
            <Setter Property="Stretch" Value="Fill" />
            <Setter Property="Grid.ZIndex" Value="100" />
            <Setter Property="StrokeThickness" Value="0.25" />
        </Style>
        <Style x:Key="horizontalLineStyle" TargetType="Line" BasedOn="{StaticResource lineStyle}">
            <Setter Property="X2" Value="1" />
            <Setter Property="VerticalAlignment" Value="Bottom" />
            <Setter Property="Grid.ColumnSpan" 
                Value="{Binding   
                            Path=ColumnDefinitions.Count,  
                            RelativeSource={RelativeSource AncestorType=Grid}}"/>
        </Style>
    </Window.Resources>
    <Window.DataContext>
        <XmlDataProvider Source="default_input.xml" XPath="xml_input" />
    </Window.DataContext>

    <DockPanel LastChildFill="True" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" >
        <StackPanel DockPanel.Dock="Top">
            <Menu Background="#f6f6f6">
                <!--run uwg -->
                <MenuItem Click="makeFile_Click" Style="{StaticResource idd}" ContextMenuService.Placement="Bottom" ContextMenuService.ShowOnDisabled="True" x:Name="makeFileMenuItem" >
                    <MenuItem.Header>
                        <StackPanel Orientation="Horizontal">
                            <ContentPresenter Content="Make Input File" />
                            <Image Width="13" Margin="4 2 0 0" Source="Images/icons/caret_down.png" />
                        </StackPanel>
                    </MenuItem.Header>
                    <MenuItem.ContextMenu>
                        <ContextMenu HorizontalAlignment="Left" x:Name="makeFileContextMenu" >
                            <MenuItem Header="_New..." Click="newXml_Click">
                                <MenuItem.Icon>
                                    <Image Source="Images/icons/menu_new.png" Width="15" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                </MenuItem.Icon>
                            </MenuItem>
                            <MenuItem Header="_Open..." Click="openXml_Click">
                                <MenuItem.Icon>
                                    <Image Source="Images/icons/menu_open.png" Width="15" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                </MenuItem.Icon>
                            </MenuItem>
                            <Separator />
                            <MenuItem Header="_Save..." Click="buttonSave_Click1" x:Name="saveButton">
                                <MenuItem.Icon>
                                    <Image Source="Images/icons/menu_save.png" Width="15" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                </MenuItem.Icon>
                            </MenuItem>
                            <MenuItem Header="_Save As..." Click="buttonSaveAs_Click" />
                            <Separator />
                            <MenuItem Header="_Exit..." Click="exit">
                                <MenuItem.Icon>
                                    <Image Source="Images/icons/close.png" Width="15" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                </MenuItem.Icon>
                            </MenuItem>
                        </ContextMenu>
                    </MenuItem.ContextMenu>
                </MenuItem>
                <!--run uwg -->
                <MenuItem Click="run_Click" Style="{StaticResource idd}" x:Name="runMenuItem">
                    <MenuItem.Header>
                        <ContentPresenter Content="Run Simulation" />
                    </MenuItem.Header>
                </MenuItem>
                <!--simulation uwg -->
                <MenuItem Click="sim_Click" Style="{StaticResource idd}" x:Name="simMenuItem">
                    <MenuItem.Header>
                        <ContentPresenter Content="View Sim. Results" />
                    </MenuItem.Header>
                </MenuItem>
                <MenuItem Click="startEditor" Style="{StaticResource idd}" x:Name="editMenuItem">
                    <MenuItem.Header>
                        <ContentPresenter Content="| Template Library |" />
                    </MenuItem.Header>
                </MenuItem>
                <MenuItem Header="Help" Click="uwgWebsite_Click" Style="{StaticResource idd}" />
            </Menu>
        </StackPanel>
        <!-- Error Output -->
        <TextBlock HorizontalAlignment="Left" Margin="-200,310,0,0" Name="textBox1" VerticalAlignment="Top" Width="112" Height="150" />
        <!-- Tab -->
        <TabControl Name="mainTabControl" Background="White" BorderBrush="#FF00B4B4" BorderThickness="0" Margin="0" Padding="0">
            <!-- construction-->
            <TabItem Header="Urban Canyon" Style="{StaticResource sec}" Name="urbanCanyonTab">
                <Grid>
                    <ScrollViewer>
                        <!-- Urban Area-->
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="260" />
                                <ColumnDefinition Width="130" />
                                <ColumnDefinition Width="174" />
                                <ColumnDefinition Width="267"/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="40" />
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="40" />
                                <RowDefinition Height="40" />
                                <RowDefinition Height="40" />
                                <!-- 0 instead of 30 as a result of image size -->
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="40"/>
                                <RowDefinition Height="Auto" />
                            </Grid.RowDefinitions>
                            <Label Content="Typologies" Style="{StaticResource SectionHeader}" Grid.Row="0" Grid.Column="0" />
                            <Line Grid.Row="0" Style="{StaticResource horizontalLineStyle}" Grid.ColumnSpan="4" />
                            <Label Content="Type" Grid.Row="0" Grid.Column="1"   />
                            <Label Content="Distribution" Grid.Row="0" Grid.Column="2" />
                            <StackPanel Style="{StaticResource PanelLeft}" Grid.Row="1" Grid.Column="0">
                                <Label Content="Typology 1" Name="typLabel1" />
                                <Label Content="Typology 2" Name="typLabel2" Visibility="Collapsed"/>
                                <Label Content="Typology 3" Name="typLabel3" Visibility="Collapsed"/>
                                <Label Content="Typology 4" Name="typLabel4" Visibility="Collapsed"/>
                                <!--add typology button-->
                            </StackPanel>
                            <Button Click="addTypology" Grid.Row="2" Grid.Column="0" HorizontalAlignment="Left" Name="addTypButton" Margin="4 0 0 0"  Style="{StaticResource specialButton}">Add Typology</Button>
                            <Button Click="minusTypology" IsEnabled="False" HorizontalAlignment="Right" Name="minusTypButton" Grid.Row="2" Grid.Column="0">Remove Typology</Button>
                            <StackPanel Style="{StaticResource PanelRight}" Grid.Row="1" Grid.Column="1">
                                <TextBox Name="typology1Type"></TextBox>
                                <TextBox Visibility="Collapsed" Name="typology2Type"></TextBox>
                                <TextBox Visibility="Collapsed"  Name="typology3Type"></TextBox>
                                <TextBox Visibility="Collapsed"  Name="typology4Type"></TextBox>
                            </StackPanel>
                            <StackPanel Style="{StaticResource PanelRight}" Grid.Row="1" Grid.Column="2" Width="30" Margin="120,7,0,0">
                                <Label Content="%" HorizontalContentAlignment="Left" Name="typPercLabel1" />
                                <Label Content="%" HorizontalContentAlignment="Left" Name="typPercLabel2" Visibility="Collapsed"/>
                                <Label Content="%" HorizontalContentAlignment="Left" Name="typPercLabel3" Visibility="Collapsed"/>
                                <Label Content="%" HorizontalContentAlignment="Left" Name="typPercLabel4" Visibility="Collapsed"/>
                            </StackPanel>
                            <StackPanel Style="{StaticResource PanelRight}" Grid.Row="1" Grid.Column="2">
                                <TextBox Visibility="Visible" Name="typology1Dist" Text="0" TextChanged="checkIfPercEquals0" ></TextBox>
                                <TextBox Visibility="Collapsed" Name="typology2Dist" Text="0" TextChanged="checkIfPercEquals0" ></TextBox>
                                <TextBox Visibility="Collapsed" Name="typology3Dist" Text="0" TextChanged="checkIfPercEquals0" ></TextBox>
                                <TextBox Visibility="Collapsed" Name="typology4Dist" Text="0" TextChanged="checkIfPercEquals0" ></TextBox>
                            </StackPanel>
                            <!--Panels from Urban Area-->
                            <Label Content="Urban Canyon Characteristics" Grid.Row="4" Grid.Column="0" Style="{StaticResource SectionHeader}"  />
                            <Line Grid.Row="4" Style="{StaticResource horizontalLineStyle}" Grid.ColumnSpan="4"/>
                            <!--<Button Grid.Row="2" Grid.Column="1" FontWeight="SemiBold" Click="importUMI" VerticalAlignment="Bottom">Import from UMI</Button>-->
                            <StackPanel Style="{StaticResource PanelLeft}" Grid.Row="5" Grid.Column="0" Grid.RowSpan="2">
                                <Label Content="Average building height [m]" />
                                <Label Content="Site coverage ratio [0 - 1]"  />
                                <Label Content="Facade to site ratio [0 - 1]" />
                                <Label Content="Tree coverage [0 - 1]" />
                                <Label Content="Non-building sensible heat [W/m^2]" />
                            </StackPanel>
                            <StackPanel Style="{StaticResource PanelRight}"  Grid.Row="5" Grid.Column="1" Grid.RowSpan="2">
                                <TextBox Name="avgBldgHeight" Text="{Binding XPath=urbanArea/averageBuildingHeight}"></TextBox>
                                <TextBox Name="siteCoverageRatio" Text="{Binding XPath=urbanArea/siteCoverageRatio}"></TextBox>
                                <TextBox Name="facadeToSiteRatio" Text="{Binding XPath=urbanArea/facadeToSiteRatio}"></TextBox>
                                <TextBox Name="treeCoverage" Text="{Binding XPath=urbanArea/treeCoverage}"></TextBox>
                                <TextBox Name="sensibleAnthroHeat" Text="{Binding XPath=urbanArea/nonBldgSensibleHeat}"></TextBox>
                            </StackPanel>
                            <!-- image-->
                            <Image HorizontalAlignment="Left" Grid.Row="5" Grid.Column="2" Source="/Images/param_diagram12.png" Margin="0 6 0 -5" Grid.ColumnSpan="2" />
                            <!-- Expander-->
                            <Expander Grid.Row="7" Header="Advanced Setting" Grid.Column="0" Grid.ColumnSpan="4" HeaderTemplate="{StaticResource expanderHeader}" >
                                <Grid>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="260" />
                                        <ColumnDefinition Width="auto" />
                                    </Grid.ColumnDefinitions>
                                    <StackPanel Style="{StaticResource PanelLeft}" Grid.Column="0">
                                        <Label Content="Non-building latent heat [W/m^2]" />
                                        <Label Content="Neighborhood characteristic length [m]" />
                                        <!--vegetation-->
                                        <Label Content="Vegetation Properties" Style="{StaticResource heading}"  />
                                        <Label Content="Latent fraction of trees [0-1]" />
                                        <Label Content="Latent fraction of grass [0-1]" />
                                        <Label Content="Albedo of vegetation [0-1]" />
                                        <Label Content="Vegetation participation begin month" />
                                        <Label Content="Vegetation participation end month" />
                                        <!--BL-->
                                        <Label Content="Boundary Layer Parameters" Style="{StaticResource heading}" />
                                        <Label Content="Daytime boundary layer height [m]"/>
                                        <Label Content="Nighttime boundary layer height [m]" />
                                        <Label Content="Reference height [m]" />
                                        <!--road-->
                                        <Label Content="Urban Road" Style="{StaticResource heading}"/>
                                        <Label Content="Assembly Template"/>
                                        <Label Content="Material"/>
                                        <Label Content="Thickness [m]"/>
                                        <Label Content="Thermal Conductivity [W/m-K]"/>
                                        <Label Content="VHC [m^3-K]"/>
                                        <Label Content="*Albedo [0-1]"/>
                                        <Label Content="*Emissivity [0-1]"/>
                                        <Label Content="Vegetation Coverage [0-1]"/>
                                    </StackPanel>
                                    <StackPanel Style="{StaticResource PanelRight}" Grid.Column="1">
                                        <TextBox Name="latentAnthroHeat" Text="{Binding XPath=urbanArea/nonBldgLatentAnthropogenicHeat}" Style="{StaticResource advSetting}"></TextBox>
                                        <TextBox Name="charLength" Text="{Binding XPath=urbanArea/charLength}" Style="{StaticResource advSetting}"></TextBox>
                                        <Label Margin="4,4,0,4" Width="auto"/>
                                        <TextBox Name="latentTrees" Text="{Binding XPath=urbanArea/treeLatent}" Style="{StaticResource advSetting}"></TextBox>
                                        <TextBox Name="latentGrass" Text="{Binding XPath=urbanArea/grassLatent}" Style="{StaticResource advSetting}"></TextBox>
                                        <TextBox Name="vegAlbedo" Text="{Binding XPath=urbanArea/vegAlbedo}" Style="{StaticResource advSetting}"></TextBox>
                                        <TextBox Name="vegStart" Text="{Binding XPath=urbanArea/vegStart}" Style="{StaticResource advSetting}"></TextBox>
                                        <TextBox Name="vegEnd" Text="{Binding XPath=urbanArea/vegEnd}" Style="{StaticResource advSetting}"></TextBox>
                                        <Label Margin="4,4,0,4" Width="auto"/>
                                        <TextBox Name="daytimeBLHeight" Text="{Binding XPath=urbanArea/daytimeBLHeight}" Style="{StaticResource advSetting}"></TextBox>
                                        <TextBox Name="nighttimeBLHeight" Text="{Binding XPath=urbanArea/nighttimeBLHeight}" Style="{StaticResource advSetting}"></TextBox>
                                        <TextBox Name="refHeight" Text="{Binding XPath=urbanArea/refHeight}" Style="{StaticResource advSetting}"></TextBox>
                                        <!--urbanRoad-->
                                        <Label Margin="4,4,0,4" Width="auto"/>
                                        <ComboBox Name="urbanRoadbox" Loaded="urbanRoadbox_load" SelectionChanged="urbanRoadbox_change"></ComboBox>
                                        <Label IsEnabled="False" Name="urbanRoadMaterial" Content="{Binding XPath=urbanArea/urbanRoad/materials/names/item}" Style="{StaticResource advSettingLabel}"></Label>
                                        <Label IsEnabled="False" Name="urbanRoadThickness" Content="{Binding XPath=urbanArea/urbanRoad/materials/thickness}" Style="{StaticResource advSettingLabel}"></Label>
                                        <Label Name="urbanRoadK" Content="{Binding XPath=urbanArea/urbanRoad/materials/thermalConductivity/item}" Style="{StaticResource advSettingLabel}"></Label>
                                        <Label Name="urbanRoadVHC" Content="{Binding XPath=urbanArea/urbanRoad/materials/volumetricHeatCapacity/item}" Style="{StaticResource advSettingLabel}"></Label>
                                        <Label Name="urbanRoadAlbedo" Content="{Binding XPath=urbanArea/urbanRoad/albedo}" Style="{StaticResource advSettingLabel}"></Label>
                                        <Label Name="urbanRoadEmissivity" Content="{Binding XPath=urbanArea/urbanRoad/emissivity}" Style="{StaticResource advSettingLabel}"></Label>
                                        <TextBox Name="urbanRoadVegFraction" Text="{Binding XPath=urbanArea/urbanRoad/vegetationCoverage}" Style="{StaticResource advSetting}"></TextBox>
                                    </StackPanel>
                                </Grid>
                            </Expander>
                        </Grid>
                    </ScrollViewer>
                </Grid>
            </TabItem>
            <!--end overview-->
            <!--typology 1 -->
            <TabItem Style="{StaticResource sec}" Visibility="Collapsed" Name="typTab1">
                <TabItem.Header>
                    <TextBlock Name="typ1Name"></TextBlock>
                </TabItem.Header>
                <Grid>
                    <ScrollViewer>
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="130" />
                                <ColumnDefinition Width="260" />
                                <ColumnDefinition Width="160" />
                                <ColumnDefinition Width="260" />
                                <ColumnDefinition Width="auto" />
                                <ColumnDefinition Width="auto" />
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <!--title-->
                                <RowDefinition Height="40" />
                                <!--wall-->
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="10"/>
                                <!--roof-->
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="10" />
                                <!--mass-->
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="10" />
                                <!-- glazing-->
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="40" />
                                <!-- bldg characteristics-->
                                <RowDefinition Height="40" />
                                <RowDefinition Height="auto" />
                                <RowDefinition Height="40" />
                                <!--adv setting-->
                                <RowDefinition Height="auto" />
                            </Grid.RowDefinitions>
                            <Label Style="{StaticResource SectionHeader}" Grid.Row="0" Grid.ColumnSpan="4" Content="Building Construction" />
