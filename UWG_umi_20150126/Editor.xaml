                                <Button Grid.Column="1" Grid.Row="9" Content="Save" Width="60" Height="20" Margin="110,4,0,0" Click="materialSave_Click"/>
                                <Button Grid.Column="1" Grid.Row="9" Content="Delete" Width="60" Height="20" Margin=" 235,4,0,0" Click="materialDelete_Click" Name="materialDeleteButton" IsEnabled="False"/>
                                <Button Grid.Column="1" Grid.Row="9" Content="New" Width="60" Height="20" Margin=" -15,4,0,0" Click="materialNew_Click"/>
                            </Grid>
