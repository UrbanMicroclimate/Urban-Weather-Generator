                                        <ComboBox Grid.Column="1" Grid.Row="5" Margin="4,4,4,4" Height="auto" Width="242" HorizontalAlignment="Left" Name="Infiltration" IsEditable="True" ItemsSource="{Binding ScheduleNameList}" SelectionChanged="heatValue_Change"/>
                                        <ComboBox Grid.Column="1" Grid.Row="6" Margin="4,4,4,4" Height="auto" Width="242" HorizontalAlignment="Left" Name="Ventilation" IsEditable="True" ItemsSource="{Binding ScheduleNameList}" SelectionChanged="heatValue_Change"/>
