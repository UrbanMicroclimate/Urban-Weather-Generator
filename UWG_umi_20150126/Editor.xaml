                                    </Grid>
                                    <Grid Grid.Row="1">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="600"/>
                                        </Grid.ColumnDefinitions>
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="30"/>
                                            <RowDefinition Height="210"/>
                                        </Grid.RowDefinitions>
                                        <Label Content="   Weekday:" Grid.Column="0" Grid.Row="0" Margin="4,0,4,0" HorizontalAlignment="Left" Foreground="#FF00B4B4" FontWeight="Bold" Width="120" Height="27"/>
                                        <Grid Grid.Row="1" Name="weekdaysGrid">
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="10"/>
                                                <ColumnDefinition Width="18"/>
