                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="30"/>
                                        <RowDefinition Height="30"/>
                                    </Grid.RowDefinitions>
                                    <Label Content="Layer" Grid.Column="1" Grid.Row="1" Margin="4,0,4,0" HorizontalAlignment="Center" FontWeight="Bold"/>
                                    <Label Content="Material" Grid.Column="2" Grid.Row="1" Margin="4,0,4,0" HorizontalAlignment="Center" FontWeight="Bold"/>
                                    <Label Content="Thickness" Grid.Column="3" Grid.Row="1" Margin="4,0,4,0" HorizontalAlignment="Center" FontWeight="Bold"/>
                                    <Label Content="Outer" Grid.Column="1" Grid.Row="2" Margin="4,0,4,0" HorizontalAlignment="Center" FontWeight="Bold"/>
                                    <Label Content="2" Grid.Column="1" Grid.Row="3" Margin="4,0,4,0" HorizontalAlignment="Center" FontWeight="Bold"/>
                                    <Label Content="3" Grid.Column="1" Grid.Row="4" Margin="4,0,4,0" HorizontalAlignment="Center" FontWeight="Bold"/>
                                    <Label Content="4" Grid.Column="1" Grid.Row="5" Margin="4,0,4,0" HorizontalAlignment="Center" FontWeight="Bold"/>
                                    <ComboBox Grid.Column="2" Grid.Row="2" Margin="4,4,4,4" Height="auto" Width="242" HorizontalAlignment="Left" Name="assemblyLayer1" IsEditable="True" SelectionChanged="assemblyValue_Change" ItemsSource="{Binding MaterialNameList}" />
                                    <ComboBox Grid.Column="2" Grid.Row="3" Margin="4,4,4,4" Height="auto" Width="242" HorizontalAlignment="Left" Name="assemblyLayer2" IsEditable="True" SelectionChanged="assemblyValue_Change" ItemsSource="{Binding MaterialNameList}" />
                                    <ComboBox Grid.Column="2" Grid.Row="4" Margin="4,4,4,4" Height="auto" Width="242" HorizontalAlignment="Left" Name="assemblyLayer3" IsEditable="True" SelectionChanged="assemblyValue_Change" ItemsSource="{Binding MaterialNameList}" />
                                    <ComboBox Grid.Column="2" Grid.Row="5" Margin="4,4,4,4" Height="auto" Width="242" HorizontalAlignment="Left" Name="assemblyLayer4" IsEditable="True" SelectionChanged="assemblyValue_Change" ItemsSource="{Binding MaterialNameList}" />
                                    <TextBox Grid.Column="3" Grid.Row="2" Margin="4,4,4,4" Height="auto" Width="92" HorizontalAlignment="Left" Name="assemblyThickness1" />
