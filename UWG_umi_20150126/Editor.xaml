                    <TreeView Name="heatTreeView" Grid.Column="0" Grid.Row="0" Height="380" Margin="20,10,10,10" ItemsSource="{Binding HeatList}" Visibility="Hidden" SelectedItemChanged="heatSelect" Background="#6600B4B4" FontWeight="Bold" >
