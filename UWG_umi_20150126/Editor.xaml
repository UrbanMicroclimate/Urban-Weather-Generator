                            <HierarchicalDataTemplate ItemsSource="{Binding ase}">
                                <TextBlock Text="{Binding Name}" />
                                <HierarchicalDataTemplate.ItemTemplate>
                                    <DataTemplate>
                                        <TextBlock Text="{Binding Name}" />
                                    </DataTemplate>
                                </HierarchicalDataTemplate.ItemTemplate>
                            </HierarchicalDataTemplate>
                        </TreeView.ItemTemplate>
                    </TreeView>
                    <TreeView Name="glazingTreeView" Grid.Column="0" Grid.Row="0" Height="380" Margin="20,10,10,10" ItemsSource="{Binding GlazingList}" Visibility="Hidden" SelectedItemChanged="glazingSelect" Background="#6600B4B4" FontWeight="Bold" >
