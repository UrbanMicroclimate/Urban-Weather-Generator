                    <TreeView Name="scheduleTreeView" Grid.Column="0" Grid.Row="0" Height="380" Margin="20,10,10,10" ItemsSource="{Binding ScheduleTypeList}" Visibility="Visible" SelectedItemChanged="scheduleSelect" Background="#6600B4B4" FontWeight="Bold" >
                        <TreeView.ItemTemplate>
                            <HierarchicalDataTemplate ItemsSource="{Binding sch}">
