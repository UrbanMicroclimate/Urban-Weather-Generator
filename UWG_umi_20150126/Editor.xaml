                                    <DataTemplate>                                     
                                        <TextBlock Text="{Binding Name}" />
                                    </DataTemplate>
                                </HierarchicalDataTemplate.ItemTemplate>
                            </HierarchicalDataTemplate>
                        </TreeView.ItemTemplate>
                    </TreeView>
                    <TreeView Name="assemblyTreeView" Grid.Column="0" Grid.Row="0" Height="380" Margin="20,10,10,10" ItemsSource="{Binding AssemblyTypeList}" Visibility="Hidden" SelectedItemChanged="assemblySelect"  Background="#6600B4B4" FontWeight="Bold" >
