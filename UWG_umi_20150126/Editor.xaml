                                <Label Content="Emissivity" Grid.Column="0" Grid.Row="6" Margin="20,0,4,0" HorizontalAlignment="Left"/>
                                <Label Content="Source" Grid.Column="0" Grid.Row="7" Margin="20,0,4,0" HorizontalAlignment="Left"/>
                                <Label Content="Comment" Grid.Column="0" Grid.Row="8" Margin="20,0,4,0" HorizontalAlignment="Left"/>
                                <TextBox Grid.Column="1" Grid.Row="1" Margin="4,4,4,4" Height="auto" Width="200" HorizontalAlignment="Left" Name="materialName" TextChanged="materialValue_Change"/>
