                                        <ComboBox Grid.Column="1" Grid.Row="2" Margin="4,4,4,4" Height="auto" Width="200" HorizontalAlignment="Left" Name="scheduleType" IsEditable="True" ItemsSource="{Binding ScheduleTypeNameList}" SelectionChanged="scheduleValue_Change"/>
                                        <TextBox Grid.Column="1" Grid.Row="3" Margin="4,4,4,4" Height="auto" Width="200" HorizontalAlignment="Left" TextWrapping="Wrap" Name="scheduleSource" TextChanged="scheduleValue_Change"/>
                                        <TextBox Grid.Column="1" Grid.Row="4" Margin="4,4,4,4" Height="auto" Width="342" HorizontalAlignment="Left" TextWrapping="Wrap" AcceptsReturn="True" Name="scheduleComment" TextChanged="scheduleValue_Change"/>
                                        <Button Grid.Column="1" Grid.Row="5" Content="Save" Width="60" Height="20" Margin="110,4,0,0" Click="scheduleSave_Click"/>
