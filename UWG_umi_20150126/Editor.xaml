                                    <Label Content="Total U-Value" Grid.Column="0" Grid.Row="3" Margin="20,0,4,0" HorizontalAlignment="Left"/>
                                    <Label Content="Total Solar Heat Gain Coefficient" Grid.Column="0" Grid.Row="4" Margin="20,0,4,0" HorizontalAlignment="Left"/>
